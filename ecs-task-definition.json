{
	"family": "tabletop-utils",
	"executionRoleArn": "arn:aws:iam::376205531064:role/ecsTaskExecutionRole",
	"networkMode": "awsvpc",
	"containerDefinitions": [{
		"name": "tabletop-utils",
		"image": "376205531064.dkr.ecr.us-west-2.amazonaws.com/tabletop-utils:latest",
		"portMappings": [{
			"containerPort": 3000,
			"hostPort": 3000,
			"protocol": "tcp"
		}],
		"essential": true,
		"secrets": [
			{
				"name": "SQL_HOST",
				"valueFrom": "arn:aws:secretsmanager:us-west-2:376205531064:secret:tabletop_utils_db_admin-DfwrgE:host::"
			},
			{
				"name": "SQL_PASS",
				"valueFrom": "arn:aws:secretsmanager:us-west-2:376205531064:secret:tabletop_utils_db_admin-DfwrgE:password::"
			}
		]
	}],
	"requiresCompatibilities": [
		"FARGATE"
	],
	"cpu": "512",
	"memory": "1024"
}